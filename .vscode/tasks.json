{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Winmerge",
			"type": "shell",
			"command": "${workspaceFolder}/.vscode/merge.ps1",
			"args": ["${workspaceFolder}", ""],
			"presentation": {
				"echo": true,
				"reveal": "silent",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false,
				"close": true
			}
		}
	]
}
